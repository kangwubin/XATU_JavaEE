<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projects.modular.api.mapper.FixMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.projects.modular.api.entity.Fix">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="user_id" property="userId" />
        <result column="create_time" property="createTime" />
        <result column="repair_time" property="repairTime" />
        <result column="status" property="status" />
        <result column="address" property="address" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", name AS "name", description AS "description", user_id AS "userId", create_time AS "createTime", repair_time AS "repairTime", status AS "status", address AS "address"
    </sql>

</mapper>
